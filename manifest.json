{
  "manifest_version": 3,

  "name": "ReScratch",
  "version": "1.0",
  "description": "Ease or change scratch.mit.edu by removing and replacing features",

  "icons": {
    "16": "extension_icon16.png",
    "32": "extension_icon32.png",
    "48": "extension_icon48.png",
    "128": "extension_icon128.png"
  },
  "content_scripts": [{
    "css": ["styles.css"],
    "matches": ["https://scratch.mit.edu/*"]
  }]
}